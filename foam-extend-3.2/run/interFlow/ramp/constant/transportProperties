FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        "constant";
    object          transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phase1
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0]  0;
    rho             rho [1 -3 0 0 0 0 0] 1;
}

phase2
{
    transportModel  Newtonian;
    nu              nu [0 2 -1 0 0 0 0]  0;
    rho             rho [1 -3 0 0 0 0 0] 0.001;
}

sigma           sigma [1 0 -2 0 0 0 0] 0.0;


